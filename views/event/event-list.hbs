<div class="container">
  <div class="row">
    {{#each eventos}}

    <div class="col-md-6">
        <p class="fs-3"> Nombre: {{name}} </p>
        <p class="fs-3"> Fecha: {{date}} </p>
        <p class="fs-3"> Direccion: {{address.street.streetName}} </p>
        <p class="fs-3"> Ciudad: {{address.city}} </p>

        <div class="d-flex">
          <a href="/eventos/editar/{{_id}}" class="btn btn-outline-dark">Editar</a>
          <form action="/eventos/{{_id}}/delete" method="POST">
            <button type="submit" class="btn btn-outline-danger">Borrar lugar</button>
          </form>
          <a href="/eventos/detalles/{{_id}}" class="btn btn-outline-dark">detalles</a>
        </div>

    </div>
    {{/each}}

  </div>
</div>
